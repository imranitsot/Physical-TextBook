{
  "name": "physical-ai-textbook",
  "version": "1.0.0",
  "description": "Physical AI Textbook - Connecting AI Agents to Physical Robot Systems - Module 2: The Digital Twin (Gazebo & Unity)",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "node --max-old-space-size=6144 ./node_modules/@docusaurus/core/bin/docusaurus.mjs start",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=6144 docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "postinstall": "chmod +x node_modules/.bin/docusaurus"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "cross-env": "^10.1.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": "22.x"
  }
}
